---
title: Home page
published: true
---
<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <title>{{ page.title }}</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="{{ site.meta_description }}">
    <meta name="keywords" content="{{ site.meta_keywords }}">

	<!-- Fonts -->
	{{ 'fonts/awesome.css' | stylesheet_tag }}
	{{ 'fonts/font-awesome.css' | stylesheet_tag }}
    <!--[if IE 7]>
      {{ 'fonts/font-awesome-ie7.min.css' | stylesheet_tag }}
    <![endif]-->
    {% include fonts %}

    <!-- Le styles -->
    {{ 'bootstrap.css' | stylesheet_tag }}
    {{ 'bootstrap-responsive.css' | stylesheet_tag }}
    {{ 'application.css' | stylesheet_tag }}

    <!-- HTML5 shim, for IE6-8 support of HTML5 elements -->
    <!--[if lt IE 9]>
      <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->

    {% inline_editor %}
  </head>

  <body style="background: url('images/technology-wallpaper2.jpg');">

    <div class="container-narrow">

      <div class="masthead">
        <ul class="nav nav-pills pull-right">
          <li><a href="/">Home</a></li>
          <li class="active"><a href="/contact">Contact</a></li>
          <li><a href="/about">Projects</a></li>
        </ul>
      <br />
			</div>


      {% block 'main' %}

      <div class="jumbotron">
        <h1 style="color: white">Get to know the person behind Technology Inferno</h1>
        <p class="lead muted">Below are various links to get ahold of the person behind Technology Inferno.</p>
				<br />
        <a class="btn btn-large btn-info" href="mailto:stephanie@nirvanadesktop.com">Email</a>&nbsp;&nbsp;
        <a class="btn btn-large btn-primary" href="https://www.facebook.com/stephanie.sunshine.520">Facebook</a>&nbsp;&nbsp;
        <a class="btn btn-large btn-warning" href="http://www.turntable.fm/express_yourself9">Turntable</a>
      </div>


      {% endblock %}

			<br />

      {% include footer %}

    </div> <!-- /container -->

    <!-- Le javascript
    ================================================== -->
    <!-- Placed at the end of the document so the pages load faster -->
    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
    {{ 'bootstrap.js' | javascript_tag }}

  </body>
</html>
